### This code is for the directors


dl <- read.csv("Director_csv.csv")

library(data.table)
dl2 <- data.table(dl)
new_dl <- dl2[, rank := frank(-AvgCS, ties.method = "dense"), by = Year]
dl_top5 <- subset(new_dl, rank < 6)[, c(2, 1, 4)]
write.csv(dl_top5, "Top5DirectorsByYear.csv", row.names = FALSE)
